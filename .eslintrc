{
  "extends": ["airbnb/base"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": 1,
    "no-console": "warn",
		"max-len": ["error", 150, {"ignoreUrls":true}, {"ignoreComments": true}],
		"comma-dangle":"off",
		"prefer-destructuring": "off",
		"no-alert":"off",
		"no-param-reassign": "off",
  }
}